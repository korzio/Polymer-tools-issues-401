{
  "entrypoint": "index.html",
  "fragments": [
    "src/my-app-app/lazy-component.html"
  ],
  "sources": [
    "src/**/*",
    "index.html",
    "src/my-app-app/lazy-component.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "bundle": {
        "rewriteUrlsInTemplates": true,
        "sourcemaps": true
      },
      "js": {
        "minify": true,
        "compile": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      }
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "moduleResolution": "node"
}